A,MAAA;AACA,OAJA,MAIA;AACA,aAAA,SAAA,GAAA;AACA,UAAA,EAAA,EAAA,IADA;AAEA,UAAA,QAAA,EAAA,EAFA;AAGA,UAAA,SAAA,EAAA,EAHA;AAIA,UAAA,SAAA,EAAA;AAJA,SAAA;AAMA,aAAA,KAAA,CAAA,SAAA,CAAA,SAAA,GAAA,MAAA;AACA;;AACA,WAAA,SAAA,GAAA,IAAA;AACA,KAhBA;AAiBA,IAAA,SAjBA,uBAiBA;AAAA;;AACA,WAAA,KAAA,CAAA,GAAA,CAAA,kBAAA,EAAA,IAAA,CAAA,gBAAA;AAAA,YAAA,IAAA,QAAA,IAAA;AACA,QAAA,KAAA,CAAA,QAAA,GAAA,IAAA,CAAA,IAAA;AACA,QAAA,KAAA,CAAA,OAAA,GAAA,KAAA;AACA,OAHA;AAIA,KAtBA;AAuBA,IAAA,aAvBA,2BAuBA;AAAA;;AACA,UAAA,KAAA,SAAA,CAAA,QAAA,CAAA,IAAA,MAAA,EAAA,EAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,UAAA;AACA,eAAA,KAAA;AACA;;AACA,UAAA,KAAA,SAAA,CAAA,SAAA,CAAA,IAAA,MAAA,EAAA,EAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,UAAA;AACA,eAAA,KAAA;AACA;;AACA,UAAA,KAAA,SAAA,CAAA,SAAA,IAAA,IAAA,EAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,UAAA;AACA,eAAA,KAAA;AACA;;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,kBAAA,EAAA,KAAA,SAAA,EAAA,IAAA,CAAA,iBAAA;AAAA,YAAA,IAAA,SAAA,IAAA;;AACA,YAAA,IAAA,CAAA,IAAA,EAAA;AACA,UAAA,MAAA,CAAA,OAAA,CAAA,OAAA,CAAA;AACA,YAAA,KAAA,EAAA,IADA;AAEA,YAAA,OAAA,EAAA,IAAA,CAAA;AAFA,WAAA;;AAIA,UAAA,MAAA,CAAA,SAAA;AACA,SANA,MAMA;AACA,UAAA,MAAA,CAAA,OAAA,CAAA,KAAA,CAAA;AACA,YAAA,KAAA,EAAA,IADA;AAEA,YAAA,OAAA,EAAA,IAAA,CAAA;AAFA,WAAA;AAIA;AACA,OAbA;AAcA,WAAA,SAAA,GAAA,KAAA;AACA,KAnDA;AAoDA,IAAA,WApDA,uBAoDA,QApDA,EAoDA;AACA,WAAA,SAAA,CAAA,SAAA,GAAA,QAAA,CAAA,IAAA;AACA,KAtDA;AAuDA,IAAA,YAvDA,wBAuDA,IAvDA,EAuDA;AAAA;;AACA,aAAA,IAAA,OAAA,CAAA,UAAA,OAAA,EAAA;AACA,YAAA,IAAA,CAAA,IAAA,GAAA,IAAA,GAAA,MAAA,CAAA,MAAA,CAAA,WAAA,EAAA;AACA,UAAA,OAAA,CAAA,IAAA,CAAA;AACA,SAHA,CAIA;;;AACA,QAAA,eAAA,CACA,kBADA,CACA,IADA,EACA,MAAA,CAAA,MAAA,CAAA,WADA,EAEA,IAFA,CAEA,UAAA,GAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA;AACA,SAJA;AAKA,OAVA,CAAA;AAWA,KAnEA;AAoEA,IAAA,aApEA,yBAoEA,OApEA,EAoEA;AACA,UAAA,IAAA,GAAA,OAAA,CAAA,SAAA,CAAA,CAAA,EAAA,CAAA,CAAA;AACA,UAAA,IAAA,GAAA,OAAA,CAAA,SAAA,CAAA,CAAA,CAAA;;AACA,UAAA,IAAA,IAAA,QAAA,EAAA;AACA,aAAA,SAAA,CAAA,EAAA,GAAA,IAAA;AACA,aAAA,YAAA,GAAA,IAAA;AACA,OAHA,MAGA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA,aAAA,SAAA,CAAA,IAAA;AACA;AACA,KA9EA;AA+EA,IAAA,UA/EA,wBA+EA;AAAA;;AACA,WAAA,KAAA,CACA,MADA,CACA,sBAAA,KAAA,SAAA,CAAA,EADA,EAEA,IAFA,CAEA,iBAAA;AAAA,YAAA,IAAA,SAAA,IAAA;;AACA,YAAA,IAAA,CAAA,IAAA,EAAA;AACA,UAAA,MAAA,CAAA,OAAA,CAAA,OAAA,CAAA;AACA,YAAA,KAAA,EAAA,IADA;AAEA,YAAA,OAAA,EAAA,IAAA,CAAA;AAFA,WAAA;;AAIA,UAAA,MAAA,CAAA,SAAA;AACA,SANA,MAMA;AACA,UAAA,MAAA,CAAA,OAAA,CAAA,KAAA,CAAA;AACA,YAAA,KAAA,EAAA,IADA;AAEA,YAAA,OAAA,EAAA,IAAA,CAAA;AAFA,WAAA;AAIA;;AACA,QAAA,MAAA,CAAA,YAAA,GAAA,KAAA;AACA,OAhBA;AAiBA;AAjGA;AAtBA,CAAA","sourcesContent":["<template>\n  <el-card class=\"main-card\">\n    <!-- 标题 -->\n    <div class=\"title\">{{ this.$route.name }}</div>\n    <div class=\"operation-container\">\n      <el-button\n        type=\"primary\"\n        size=\"small\"\n        icon=\"el-icon-plus\"\n        @click=\"openModel(null)\"\n      >\n        新建页面\n      </el-button>\n    </div>\n    <!-- 相册列表 -->\n    <el-row class=\"page-container\" :gutter=\"12\" v-loading=\"loading\">\n      <!-- 空状态 -->\n      <el-empty v-if=\"pageList.length == 0\" description=\"暂无页面\" />\n      <el-col v-for=\"item of pageList\" :key=\"item.id\" :md=\"6\">\n        <div class=\"page-item\">\n          <!-- 相册操作 -->\n          <div class=\"page-opreation\">\n            <el-dropdown @command=\"handleCommand\">\n              <i class=\"el-icon-more\" style=\"color:#fff\" />\n              <el-dropdown-menu slot=\"dropdown\">\n                <el-dropdown-item :command=\"'update' + JSON.stringify(item)\">\n                  <i class=\"el-icon-edit\" />编辑\n                </el-dropdown-item>\n                <el-dropdown-item :command=\"'delete' + item.id\">\n                  <i class=\"el-icon-delete\" />删除\n                </el-dropdown-item>\n              </el-dropdown-menu>\n            </el-dropdown>\n          </div>\n          <el-image fit=\"cover\" class=\"page-cover\" :src=\"item.pageCover\" />\n          <div class=\"page-name\">{{ item.pageName }}</div>\n        </div>\n      </el-col>\n    </el-row>\n    <!-- 新增模态框 -->\n    <el-dialog :visible.sync=\"addOrEdit\" width=\"35%\" top=\"10vh\">\n      <div class=\"dialog-title-container\" slot=\"title\" ref=\"pageTitle\" />\n      <el-form label-width=\"80px\" size=\"medium\" :model=\"pageForum\">\n        <el-form-item label=\"页面名称\">\n          <el-input style=\"width:220px\" v-model=\"pageForum.pageName\" />\n        </el-form-item>\n        <el-form-item label=\"页面标签\">\n          <el-input style=\"width:220px\" v-model=\"pageForum.pageLabel\" />\n        </el-form-item>\n        <el-form-item label=\"页面封面\">\n          <el-upload\n            class=\"upload-cover\"\n            drag\n            :show-file-list=\"false\"\n            action=\"/api/admin/config/images\"\n            multiple\n            :before-upload=\"beforeUpload\"\n            :on-success=\"uploadCover\"\n          >\n            <i class=\"el-icon-upload\" v-if=\"pageForum.pageCover == ''\" />\n            <div class=\"el-upload__text\" v-if=\"pageForum.pageCover == ''\">\n              将文件拖到此处，或<em>点击上传</em>\n            </div>\n            <img\n              v-else\n              :src=\"pageForum.pageCover\"\n              width=\"360px\"\n              height=\"180px\"\n            />\n          </el-upload>\n        </el-form-item>\n      </el-form>\n      <div slot=\"footer\">\n        <el-button @click=\"addOrEdit = false\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"addOrEditPage\">\n          确 定\n        </el-button>\n      </div>\n    </el-dialog>\n    <!-- 删除对话框 -->\n    <el-dialog :visible.sync=\"isdeletePage\" width=\"30%\">\n      <div class=\"dialog-title-container\" slot=\"title\">\n        <i class=\"el-icon-warning\" style=\"color:#ff9900\" />提示\n      </div>\n      <div style=\"font-size:1rem\">是否删除该页面？</div>\n      <div slot=\"footer\">\n        <el-button @click=\"isdeletePage = false\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"deletePage\">\n          确 定\n        </el-button>\n      </div>\n    </el-dialog>\n  </el-card>\n</template>\n\n<script>\nimport * as imageConversion from \"image-conversion\";\nexport default {\n  created() {\n    this.listPages();\n  },\n  data: function() {\n    return {\n      keywords: \"\",\n      loading: true,\n      current: 1,\n      size: 8,\n      count: 0,\n      isdeletePage: false,\n      addOrEdit: false,\n      pageForum: {\n        id: null,\n        pageName: \"\",\n        pageLabel: \"\",\n        pageCover: \"\"\n      },\n      pageList: []\n    };\n  },\n  methods: {\n    openModel(item) {\n      if (item) {\n        console.log(item);\n        this.pageForum = JSON.parse(item);\n        this.$refs.pageTitle.innerHTML = \"修改页面\";\n      } else {\n        this.pageForum = {\n          id: null,\n          pageName: \"\",\n          pageLabel: \"\",\n          pageCover: \"\"\n        };\n        this.$refs.pageTitle.innerHTML = \"新建页面\";\n      }\n      this.addOrEdit = true;\n    },\n    listPages() {\n      this.axios.get(\"/api/admin/pages\").then(({ data }) => {\n        this.pageList = data.data;\n        this.loading = false;\n      });\n    },\n    addOrEditPage() {\n      if (this.pageForum.pageName.trim() == \"\") {\n        this.$message.error(\"页面名称不能为空\");\n        return false;\n      }\n      if (this.pageForum.pageLabel.trim() == \"\") {\n        this.$message.error(\"页面标签不能为空\");\n        return false;\n      }\n      if (this.pageForum.pageCover == null) {\n        this.$message.error(\"页面封面不能为空\");\n        return false;\n      }\n      this.axios.post(\"/api/admin/pages\", this.pageForum).then(({ data }) => {\n        if (data.flag) {\n          this.$notify.success({\n            title: \"成功\",\n            message: data.message\n          });\n          this.listPages();\n        } else {\n          this.$notify.error({\n            title: \"失败\",\n            message: data.message\n          });\n        }\n      });\n      this.addOrEdit = false;\n    },\n    uploadCover(response) {\n      this.pageForum.pageCover = response.data;\n    },\n    beforeUpload(file) {\n      return new Promise(resolve => {\n        if (file.size / 1024 < this.config.UPLOAD_SIZE) {\n          resolve(file);\n        }\n        // 压缩到200KB,这里的200就是要压缩的大小,可自定义\n        imageConversion\n          .compressAccurately(file, this.config.UPLOAD_SIZE)\n          .then(res => {\n            resolve(res);\n          });\n      });\n    },\n    handleCommand(command) {\n      const type = command.substring(0, 6);\n      const data = command.substring(6);\n      if (type == \"delete\") {\n        this.pageForum.id = data;\n        this.isdeletePage = true;\n      } else {\n        console.log(data);\n        this.openModel(data);\n      }\n    },\n    deletePage() {\n      this.axios\n        .delete(\"/api/admin/pages/\" + this.pageForum.id)\n        .then(({ data }) => {\n          if (data.flag) {\n            this.$notify.success({\n              title: \"成功\",\n              message: data.message\n            });\n            this.listPages();\n          } else {\n            this.$notify.error({\n              title: \"失败\",\n              message: data.message\n            });\n          }\n          this.isdeletePage = false;\n        });\n    }\n  }\n};\n</script>\n\n<style scoped>\n.page-cover {\n  position: relative;\n  border-radius: 4px;\n  width: 100%;\n  height: 170px;\n}\n.page-name {\n  text-align: center;\n  margin-top: 0.5rem;\n}\n.page-item {\n  position: relative;\n  cursor: pointer;\n  margin-bottom: 1rem;\n}\n.page-opreation {\n  position: absolute;\n  z-index: 1000;\n  top: 0.5rem;\n  right: 0.8rem;\n}\n</style>\n"],"sourceRoot":"src/views/page"}]}